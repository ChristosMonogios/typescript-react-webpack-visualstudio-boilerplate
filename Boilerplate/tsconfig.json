{
  "compilerOptions": {
    "outDir": "./distribution/",
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "noEmitHelpers": true
  },
  "include": [
    "./scripts/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}